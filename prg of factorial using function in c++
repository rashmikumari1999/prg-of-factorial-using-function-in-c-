#include <iostream>
using namespace std;
int factorial(int);
int main()
{
    int f,num;
    cout<<"enter a number";
    cin>>num;
    f=factorial(num);
cout<<"factorial of num"<<"is"<<num <<f;
}
int factorial(int n)
{
    int i,f=1;
    for(i=1;i<=n;i++)
    {
        f=f*i;
    }
    return f;
}

